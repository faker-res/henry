{
  "parserOptions": {
    "ecmaVersion": 6
//    "sourceType": "module",
//    "ecmaFeatures": {
//      "jsx": true
//    }
  },
  "extends": ["eslint:recommended", "google"],
  "rules": {
    // Additional, per-project rules...
    "no-console": "off",
    "max-len": [1, 120, 4],
    "quote-props": "off",
    "comma-dangle": "off",
    "arrow-parens": "off",
    "quotes": "off"
  },
  "env": {
    "amd": true,
    "commonjs": true,
    "node": true,
    "mocha": true
  }
}