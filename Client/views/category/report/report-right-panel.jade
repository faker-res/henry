                    label.control-label {{'Start date' | translate}}
                    .startTime
                div
                    label.control-label {{'End date' | translate}}
                    .endTime
                div
                    label &nbsp;
                    button.btn.btn-success(ng-click='vm.searchTopupRecord(true)') {{'SEARCH' | translate}}
