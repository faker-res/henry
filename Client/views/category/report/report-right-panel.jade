                    label {{'Business Acc/ Bank Acc' | translate}}
                    select.form-control.merchantNoList(ng-change='vm.groupByIfAliPayLine()', ng-model='vm.queryTopup.merchantNoData', ng-options='a as a.name group by a.merchantTypeName for a in vm.merchantCloneList' , multiple, bsp, alipay-line-group, data-actions-box='true')
