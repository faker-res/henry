form(name="form_new_platform" ,novalidate)

    .well(ng-show="vm.selectedPlatformDetailTab != 'system-settlement'")
        .row
            div(name="backstage-settings", ng-show="vm.selectedPlatformDetailTab == 'backstage-settings'")
                div.form-group.col-md-6
                    label.control-label *{{"PLATFORM_NAME" | translate}}
                    input.form-control(type="text", placeholder="{{'PLATFORM_NAME' | translate}}", ng-model="vm.showPlatform.name", ng-disabled="vm.isNotAllowEdit", required)
                div.form-group.col-md-6
                    label.control-label *{{"PLATFORM_ID" | translate}}
                    div
                    input.form-control(type="text", placeholder="{{'PLATFORM_ID' | translate}}", ng-model="vm.showPlatform.platformId", ng-disabled="!vm.isCreateNewPlatform")
                div.form-group.col-md-6
                    label.control-label *{{"PLATFORM_SHORTFORM" | translate}}
                    input.form-control(type="text", placeholder="{{'PLATFORM_CODE' | translate}}", ng-model="vm.showPlatform.code", ng-disabled="vm.isNotAllowEdit", required)
                div.form-group.col-md-7
                div.form-group.col-md-6
                    label.control-label {{"PLATFORM_PLAYER_PREFIX" | translate}}
                    input.form-control(type="text", placeholder="{{'PLATFORM_PREFIX' | translate}}", ng-model="vm.showPlatform.prefix", ng-disabled="vm.isNotAllowEdit")
                div.form-group.col-md-6
                    label.control-label {{"PLATFORM_PARTNER_PREFIX" | translate}}
                    input.form-control(type="text", placeholder="{{'PLATFORM_PARTNER_PREFIX' | translate}}", ng-model="vm.showPlatform.partnerPrefix", ng-disabled="vm.isNotAllowEdit")
                div.form-group.col-md-6
                    label.control-label {{"DemoPlayerPrefix" | translate}}
                    div
                    select(ng-model='vm.showPlatform.demoPlayerPrefix', ng-disabled="vm.isNotAllowEdit", style='height:30px')
                        option(ng-repeat='(i,v) in vm.alphabetArr', value='{{v}}') {{v}}
                div.form-group.col-md-6
                    label.control-label {{"DEMO_PLAYER_DEFAULT_CREDIT" | translate}}
                    div
                    input.form-control(type="text", placeholder="{{'DEMO_PLAYER_DEFAULT_CREDIT' | translate}}", ng-model="vm.showPlatform.demoPlayerDefaultCredit", ng-disabled="vm.isNotAllowEdit")
                div.form-group.col-md-6
                    label.control-label {{"PUSH_NOTIFICATION_APP_KEY" | translate}}
                    input.form-control(type="text", ng-model="vm.showPlatform.jiguangAppKey", ng-disabled="vm.isNotAllowEdit")
                div.form-group.col-md-6
                    label.control-label {{"PUSH_NOTIFICATION_MASTER_KEY" | translate}}
                    input.form-control(type="text", ng-model="vm.showPlatform.jiguangMasterKey", ng-disabled="vm.isNotAllowEdit")
                div.form-group.col-md-6
                    label.control-label {{"LIVE800 COMPANYID" | translate}}
                    input.form-control(type="text", placeholder="{{'live800CompanyId' | translate}}",ng-blur="vm.isValidCompanyId(vm.showPlatform.live800CompanyIdTXT)", ng-model= "vm.showPlatform.live800CompanyIdTXT", ng-disabled="vm.isNotAllowEdit")
                    div(ng-if="vm.live800CompanyIdError", style='color:red') {{vm.live800CompanyIdError}}
                div.form-group.col-md-6
                    label.control-label {{"CS Department" | translate}}
                    input.form-control(type="text", ng-model="vm.showPlatform.csDepartmentTXT", ng-blur="vm.isValidCSDepartment(vm.showPlatform.csDepartmentTXT)", ng-disabled="vm.isNotAllowEdit")
                    div(ng-if="vm.csDepartmentError", style='color:red') {{vm.csDepartmentError}}
                div.form-group.col-md-6
                    label.control-label {{"QI Department" | translate}}
                    input.form-control(type="text", ng-model="vm.showPlatform.qiDepartmentTXT", ng-blur="vm.isValidQIDepartment(vm.showPlatform.qiDepartmentTXT)", ng-disabled="vm.isNotAllowEdit")
                    div(ng-if="vm.qiDepartmentError", style='color:red') {{vm.qiDepartmentError}}



