#modalSkinColor.modal.fade(tabindex='-1', role='dialog', data-backdrop='static', keyboard=false,aria-labelledby='myModalLabel', aria-hidden='true')
    .modal-dialog.modal-medium
        .modal-content
            .modal-header
                a.fa.fa-times.close(type='button', data-dismiss='modal')
                h4.modal-title {{ vm.curKey | translate}}
            .modal-body
                .panel.panel-primary
                    .panel-body
                        .row
                            .col-md-12
                                ul.nav.nav-tabs
                                    li.active
                                        a(data-toggle="tab" href="#solids") {{ "Single_Color" | translate}}
                                    li
                                        a(data-toggle="tab" href="#gradients") {{ "Gradient_Color" | translate}}
                                div.tab-content.colorTab
                                    div#solids.tab-pane.fade.in.active.col-xs-5
                                        label {{ "Color" | translate}} <br>
                                            input(type="color" value="#ff0000" ng-model="vm.frontEndUrlConfig[vm.curDevice][vm.curKey]", ng-change="vm.skinColor(vm.frontEndUrlConfig[vm.curDevice][vm.curKey], null, null, null)")
                                    div#gradients.tab-pane.fade.col-xs-5
                                        label {{ "Color" | translate}} <br>
                                            input(type="color" value="#ff0000" ng-model="vm.frontEndUrlConfig[vm.curDevice][vm.curKey+'_g1']", ng-change="vm.skinColor(null, vm.frontEndUrlConfig[vm.curDevice][vm.curKey+'_g1'], vm.frontEndUrlConfig[vm.curDevice][vm.curKey+'_g2'], vm.frontEndUrlConfig[vm.curDevice][vm.curKey+'_deg'])")
                                        <br>
                                        label {{ "Color" | translate}} <br>
                                            input(type="color" value="#ff0000" ng-model="vm.frontEndUrlConfig[vm.curDevice][vm.curKey+'_g2']", ng-change="vm.skinColor(null, vm.frontEndUrlConfig[vm.curDevice][vm.curKey+'_g1'], vm.frontEndUrlConfig[vm.curDevice][vm.curKey+'_g2'], vm.frontEndUrlConfig[vm.curDevice][vm.curKey+'_deg'])")
                                        <br>
                                        label {{ "Degrees" | translate}} <br>
                                            input(type="number", ng-model="vm.frontEndUrlConfig[vm.curDevice][vm.curKey+'_deg']", ng-change="vm.skinColor(null, vm.frontEndUrlConfig[vm.curDevice][vm.curKey+'_g1'], vm.frontEndUrlConfig[vm.curDevice][vm.curKey+'_g2'], vm.frontEndUrlConfig[vm.curDevice][vm.curKey+'_deg'])")
                                    div.bigColorDisplay.col-xs-7(ng-style='{"background-color": vm.frontEndUrlConfig[vm.curDevice][vm.curKey], "background-image": vm.frontEndUrlConfig[vm.curDevice][vm.curKey]}')


            //.modal-footer
            //    button.btn-sm.margin-right-5.btn.btn-primary.col-md-2(data-dismiss='modal' ng-click='vm.deleteRewardPointsEvent(vm.deletingRewardPointsEvent)') {{"DELETE" | translate}}
            //    button.btn-sm.margin-right-5.btn.btn-warning.col-md-2(data-dismiss='modal') {{"CANCEL" | translate}}
