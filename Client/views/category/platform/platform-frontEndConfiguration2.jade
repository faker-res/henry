style.
    .connected-sortable {
        /*margin: 0 auto;*/
        list-style: none;
        padding: 0px;
        /*width: 90%;*/
        min-height: 90%;
    }
    li.draggable-item {
        width: inherit;
        padding: 0.5em;
        background-color: #f5f5f5;
        border-radius: 4px;
        margin: 1em 0;
        border: 1px solid rgb(190, 190, 190);
        text-align: center;
        position: relative;
        -webkit-transition: transform .25s ease-in-out;
        -moz-transition: transform .25s ease-in-out;
        -o-transition: transform .25s ease-in-out;
        transition: transform .25s ease-in-out;

        -webkit-transition: box-shadow .25s ease-in-out;
        -moz-transition: box-shadow .25s ease-in-out;
        -o-transition: box-shadow .25s ease-in-out;
        transition: box-shadow .25s ease-in-out;
    }
    /* styles during drag */
    li.draggable-item.ui-sortable-helper {
        background-color: #e5e5e5;
        -webkit-box-shadow: 0 0 8px rgba(53, 41, 41, .8);
        -moz-box-shadow: 0 0 8px rgba(53, 41, 41, .8);
        box-shadow: 0 0 8px rgba(53, 41, 41, .8);
        transform: scale(1.015);
        z-index: 100;
    }
    li.draggable-item.ui-sortable-placeholder {
        background-color: #ddd;
        -moz-box-shadow: inset 0 0 10px #000000;
        -webkit-box-shadow: inset 0 0 10px #000000;
        box-shadow: inset 0 0 10px #000000;
    }
    .tab-btngroup {
        display: inline-block;
        position: absolute;
        top: 50%;
        right: 0;
        z-index: 100;
        transform: translateY(-50%);
    }
    .btn-delete {
        border-radius: 100%!important;
        margin: 0 .5em;
    }
.row
    .col-md-12
        button.btn.btn-primary.common-button(ng-class='{active: vm.selectedFrontEndSettingTab === "popularRecommendation"}', ng-click='vm.frontEndSettingTabClicked("popularRecommendation")')
            span {{"POPULAR_RECOMMENDATION" | translate}}
        button.btn.btn-primary.common-button(ng-class='{active: vm.selectedFrontEndSettingTab === "rewardPointClarification"}', ng-click='vm.frontEndSettingTabClicked("rewardPointClarification")')
            span {{"REWARD_POINT_CLARIFICATION" | translate}}
        button.btn.btn-primary.common-button(ng-class='{active: vm.selectedFrontEndSettingTab === "carouselConfiguration"}', ng-click='vm.frontEndSettingTabClicked("carouselConfiguration")')
            span {{"CAROUSEL_CONFIGURATION" | translate}}
        button.btn.btn-primary.common-button(ng-class='{active: vm.selectedFrontEndSettingTab === "partnerCarouselConfiguration"}', ng-click='vm.frontEndSettingTabClicked("partnerCarouselConfiguration")')
            span {{"PARTNER_CAROUSEL_CONFIGURATION" | translate}}
        button.btn.btn-primary.common-button(ng-class='{active: vm.selectedFrontEndSettingTab === "popUpAdvertisement"}', ng-click='vm.frontEndSettingTabClicked("popUpAdvertisement")')
            span {{"POP_UP_ADVERTISEMENT" | translate}}
        button.btn.btn-primary.common-button(ng-class='{active: vm.selectedFrontEndSettingTab === "urlConfiguration"}', ng-click='vm.frontEndSettingTabClicked("urlConfiguration")')
            span {{"URL_CONFIGURATION" | translate}}
        button.btn.btn-primary.common-button(ng-class='{active: vm.selectedFrontEndSettingTab === "skinManagement"}', ng-click='vm.frontEndSettingTabClicked("skinManagement")')
            span {{"SKIN_MANAGEMENT" | translate}}
        button.btn.btn-primary.common-button(ng-class='{active: vm.selectedFrontEndSettingTab === "rewardSetting"}', ng-click='vm.frontEndSettingTabClicked("rewardSetting")')
            span {{"REWARD_SETTING" | translate}}

    .col-md-12
        .panel.panel-primary
            .panel-heading.panel-heading-sm
                label {{"FrontendConfiguration" | translate}}
            .panel-body.common-right-sub-panel-body(ng-if="!vm.curPlatformText")
            .panel-body.common-right-sub-panel-body(ng-if="vm.curPlatformText")
                .text-center(ng-if='!vm.selectedFrontEndSettingTab')
                    label.text-danger {{'Please select a type' | translate}}

                .row
                    .col-md-12
                        .well.col-xs-12
                            .search-filters-item
                                label {{'PRODUCT_NAME' | translate}}
                                select.form-control(ng-model='vm.filterFrontEndSettingPlatform', ng-options='v._id as v.name$ for v in vm.allPlatformData', ng-change='vm.frontEndSettingPlatform()')
                .text-center(ng-if='!vm.filterFrontEndSettingPlatform')
                    label.text-danger {{"NO_PLATFORM_MESSAGE" | translate}}

                // Tab - popular recommendation
                .col-md-12(ng-show='vm.selectedFrontEndSettingTab == "popularRecommendation" && vm.filterFrontEndSettingPlatform && checkViewPermission("Platform", "FrontEndConfiguration", "popularRecommendation")')
                    include frontEndConfiguration-popularRecommendation

                // Tab - reward point briefing
                .col-md-12(ng-if='vm.selectedFrontEndSettingTab=="rewardPointClarification" && vm.filterFrontEndSettingPlatform && checkViewPermission("Platform", "FrontEndConfiguration", "rewardPointClarification")')
                    include frontEndConfiguration-rewardPointClarification

                // Tab - url Configuration
                .col-md-12(ng-if='vm.selectedFrontEndSettingTab=="carouselConfiguration" && vm.filterFrontEndSettingPlatform && checkViewPermission("Platform", "FrontEndConfiguration", "carouselConfiguration")')
                    include frontEndConfiguration-carousel

                .col-md-12(ng-if='vm.selectedFrontEndSettingTab=="partnerCarouselConfiguration" && vm.filterFrontEndSettingPlatform && checkViewPermission("Platform", "FrontEndConfiguration", "carouselConfiguration")')
                    include frontEndConfiguration-carousel

                // Tab - pop-up advertisement
                .col-md-12(ng-if='vm.selectedFrontEndSettingTab=="popUpAdvertisement" && vm.filterFrontEndSettingPlatform && checkViewPermission("Platform", "FrontEndConfiguration", "popUpAdvertisement")')
                    include frontEndConfiguration-popUpAdvertisement

                // Tab - url Configuration
                .col-md-12(ng-if='vm.selectedFrontEndSettingTab=="urlConfiguration" && vm.filterFrontEndSettingPlatform && checkViewPermission("Platform", "FrontEndConfiguration", "urlConfiguration")')
                    include frontEndConfiguration-url

                // Tab - skin management
                .col-md-12(ng-if='vm.selectedFrontEndSettingTab=="skinManagement" && vm.filterFrontEndSettingPlatform && checkViewPermission("Platform", "FrontEndConfiguration", "skinManagement")')
                    include frontEndConfiguration-skinManagement

                // Tab - Event Setting
                .col-md-12(ng-if='vm.selectedFrontEndSettingTab=="rewardSetting" && vm.filterFrontEndSettingPlatform && checkViewPermission("Platform", "FrontEndConfiguration", "rewardSetting")')
                    include frontEndConfiguration-rewardSetting

