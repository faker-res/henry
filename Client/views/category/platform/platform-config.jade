            .modal-body
                table.simple-table(style='max-width:100%')
                    thead
                        tr
                            th {{'RECIPIENT' | translate}}
                            th {{'USER_NAME' | translate}}
                            th {{'EMAIL' | translate}}
                            th {{'DEPARTMENT' | translate}}
                            th {{'ROLE' | translate}}
                            th {{'EMAIL_EXECUTE_PERMISSION' | translate}}
                    tbody
                        tr(ng-repeat='(i,admin) in vm.adminList | unique: "_id"' ng-attr-id="{{ 'largeWithdrawPartnerRow' + i }}")
                            td
                                input(type='checkbox' ng-model='vm.largeWithdrawPartnerCheckRecipient[i]' ng-click='vm.setLargeWithdrawPartnerRecipient(admin._id, vm.largeWithdrawPartnerCheckRecipient[i], i)')
                            td
                                text {{admin.adminName}}
                            td
                                text {{admin.email}}
                            td
                                text {{admin.departmentName | translate}}
                            td
                                text {{vm.getAdminRoleNameString(admin.roleName) | translate}}
                            td
                                input(ng-if='vm.largeWithdrawPartnerCheckRecipient[i]' type='checkbox' ng-model='vm.largeWithdrawPartnerCheckReviewer[i]' ng-click='vm.setLargeWithdrawPartnerReviewer(admin._id, vm.largeWithdrawPartnerCheckReviewer[i], i)')
                                text(ng-if='!vm.largeWithdrawPartnerCheckRecipient[i]') {{"-"}}

            .modal-footer
                button.btn.btn-primary.common-button(data-dismiss='modal' ng-click='vm.updateLargeWithdrawalPartnerRecipient()') {{"CONFIRM" | translate}}
                button.btn.btn-success.common-button(data-dismiss='modal' ng-click='vm.configTabClicked("largeWithdrawalSetting")') {{"CLOSE" | translate}}


