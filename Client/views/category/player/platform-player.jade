                input.form-control(type="number" ng-model="vm.playerAdvanceSearchQuery.creditAmountTwo" style="width:80px;display:inline-block" ng-if="vm.playerAdvanceSearchQuery.creditOperator === 'range'" ng-change='vm.getPlayersByAdvanceQueryDebounced(vm.playerAdvanceSearchQuery)')
