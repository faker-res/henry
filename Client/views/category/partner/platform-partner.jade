                                    input.form-control.colorRed(type='number', ng-model='val.amount', ng-change='vm.getSumTotalTransferAmount(vm.downlinePlayersData);vm.disableTransferPartnerCreditToPlayer(val.amount);' ng-disabled='!vm.isEditingTransferPartnerCredit')
                                td
