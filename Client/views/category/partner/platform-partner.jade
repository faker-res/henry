                a.fa.fa-times.close(type='button', data-dismiss='modal')
                h4.modal-title {{"Transfer Partner Credit To Player" | translate}}: {{vm.selectedSinglePartner.partnerName}}

            .modal-body
                div.col-sm-12
                    table.simple-table
                        tbody
