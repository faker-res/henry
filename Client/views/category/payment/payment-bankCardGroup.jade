                        .panel-heading.panel-heading-sm(data-toggle='collapse', data-parent="#bankList", href='#includedBanks', ng-click='vm.toggleBankType("include")')
                            h4.panel-title
                                text {{'Included Bankcards' | translate}}
                                span(ng-if='vm.includedBanks') &nbsp;:&nbsp;({{vm.includedBanks.length}})
                                text.blink.margin-left-5.text-danger(ng-if='!vm.includedBanks') {{'loading bankcards' | translate}}...
                        #includedBanks.panel-collapse.collapse.in
                            .panel-body(style='height:250px;overflow-y:auto')
                                a.platformBankcardDiv(ng-repeat='(i,v) in vm.includedBanks', class='{{vm.highlightBank[v.accountNumber]}}', ng-click='vm.bankClicked(i,v,"in")')
