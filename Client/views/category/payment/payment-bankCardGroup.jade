style.
    .bg-secondary {
        background-color: #868E96;
    }
.col-sm-12(ng-if='!vm.curPlatformText')
    .text-center
        label.text-danger {{"NO_PLATFORM_MESSAGE" | translate}}
.row(ng-if='vm.curPlatformText')
    .col-md-2
        .panel.panel-primary(style="min-height:610px")
            .panel-heading.panel-heading-sm
                label {{"Bankcard Group" | translate}}
            .panel-body
                div
                    input#FPMSPaymentGroup(type='radio' value='FPMS' ng-model="vm.groupUsed" ng-change="vm.getAllBankCard();vm.loadBankCardGroup()")
                    label.radio-space(for='FPMSPaymentGroup') {{"FPMS Group" | translate}}
                    input#PMSPaymentGroup(type='radio' value='PMS' ng-model="vm.groupUsed" ng-change="vm.loadBankCardGroup()")
                    label.radio-space(for='PMSPaymentGroup') {{"PMS Group" | translate}}
                div(ng-if='!vm.platformBankCardGroupList || vm.platformBankCardGroupList.length==0', style="height: 50px; overflow-y: auto;")
                    label.text-danger {{"NO_BANKCARD_GROUP_MESSAGE" | translate}}
                //div#bankCardGroupTree(style="height: 430px; overflow-y: auto;")
                ul.list-group.treeview(ng-if='vm.platformBankCardGroupList.length > 0', style="height: 430px; overflow-y: auto;")
                    li.list-group-item(ng-repeat='(i,bankCardGroup) in vm.platformBankCardGroupList', ng-click='vm.bankCardGroupClicked(i,bankCardGroup)', ng-class='{active: vm.SelectedBankCardGroupNode === bankCardGroup}')
                        text {{bankCardGroup.name}}
                        i.fa.fa-star.pull-right.margin-right-5(ng-show='bankCardGroup.bDefault')
                div.paymentCard
                        label {{"PAYMENT_CARD_MODE" | translate}}
                        br
                        input#bankCardFPMSGroupSetting(type='radio' value='false' ng-model="vm.bankCardGroupPMS" ng-disabled="!checkViewPermission('Payment', 'BankCardGroup', 'UpdateCardGroupType')")
                        label.radio-space(for='bankCardFPMSGroupSetting') {{"FPMS Group" | translate}}
                        input#bankCardPMSGroupSetting(ng-if='!vm.selectedPlatform.data.financialSettlement.financialSettlementToggle' type='radio' value='true' ng-model="vm.bankCardGroupPMS" ng-disabled="!checkViewPermission('Payment', 'BankCardGroup', 'UpdateCardGroupType')")
                        label.radio-space(ng-if='!vm.selectedPlatform.data.financialSettlement.financialSettlementToggle' for='bankCardPMSGroupSetting') {{"PMS Group" | translate}}
                        br
                        button.btn.btn-primary(ng-click="vm.updateIsPMSGroup(vm.bankCardGroupPMS, 'bankCardGroupIsPMS')" ng-if="checkViewPermission('Payment', 'BankCardGroup', 'UpdateCardGroupType')") {{"CONFIRMATION" | translate}}
                br
                .btn-group.text-center(role="group")
                    button.btn.btn-primary.vertical-button-group(ng-if="vm.selectedPlatform.data.financialSettlement && vm.selectedPlatform.data.financialSettlement.financialSettlementToggle && checkViewPermission('Payment', 'BankCardGroup', 'CreateBankCard')", data-toggle="modal", ng-click='vm.newBankCardAcc={};', data-target='#modalAddNewBankCard')
                        img.img-create.
                        text {{"CreateBankCard" | translate}}
                    button.btn.btn-primary.vertical-button-group(ng-if="checkViewPermission('Payment', 'BankCardGroup', 'Create') && vm.groupUsed != 'PMS'", data-toggle="modal", ng-click='vm.newBankCardGroup={};vm.newBankCardGroup.parent=vm.SelectedBankCardGroupNode.id', data-target='#modalAddBankCardGroup')
                        img.img-create
                        text {{"ADD" | translate}}
                    button.btn.btn-primary.vertical-button-group(ng-if="checkViewPermission('Payment', 'BankCardGroup', 'Delete') && vm.groupUsed != 'PMS'", ng-disabled="!vm.selectedPlatform || !vm.SelectedBankCardGroupNode", data-toggle="modal", data-target='#modalRemoveBankCardGroup', ng-click='')
                        img.img-delete
                        text {{"DELETE" | translate}}
                    button.btn.btn-primary.vertical-button-group(ng-if="checkViewPermission('Payment', 'BankCardGroup', 'Update') && vm.groupUsed != 'PMS'", ng-disabled="!vm.selectedPlatform || !vm.SelectedBankCardGroupNode", data-toggle="modal", data-target='#modalRenameBankCardGroup', ng-click='vm.initRenameBankCardGroup()')
                        img.img-edit
                        text {{"EDIT" | translate}}
                    button.btn.btn-primary.vertical-button-group(ng-if="checkViewPermission('Payment', 'BankCardGroup', 'Update') && vm.groupUsed != 'PMS'", ng-disabled="!vm.selectedPlatform || !(vm.platformBankCardGroupList.length>0)", data-toggle="modal", data-target='#modalSetBankCardGroupDefault')
                        img.img-edit
                        text {{"Default" | translate}}
                    button.btn.btn-primary.vertical-button-group(ng-if="checkViewPermission('Payment', 'BankCardGroup', 'AddPlayer') && vm.groupUsed != 'PMS'", ng-disabled="!vm.selectedPlatform || !vm.SelectedBankCardGroupNode", data-toggle="modal", data-target='#modalAddPlayersToBankCardGroup', ng-click='vm.preparePlayerToGroupDialog("bankcardGroup","#playertoGroupDataTable")')
                        img.img-create
                        text {{"ADD_PLAYER" | translate}}
                    button.btn.btn-primary.vertical-button-group(ng-if="checkViewPermission('Payment', 'BankCardGroup', 'AddAllPlayer') && vm.groupUsed != 'PMS'", ng-disabled="!vm.selectedPlatform || !vm.SelectedBankCardGroupNode", data-toggle="modal", data-target='#modalAddAllPlayersToBankCardGroup', ng-click='vm.addAllPlayerToBankCardResult=""')
                        img.img-create
                        text {{"ADD_ALL_PLAYER" | translate}}
    .col-md-10(style="padding-left:0;padding-right:15px")
        .panel.panel-primary(style="min-height:610px")
            .panel-heading.panel-heading-sm
                label {{"Bankcard Group" | translate}} : {{vm.SelectedBankCardGroupNode.name}} ({{vm.SelectedBankCardGroupNode.displayName}})
            .panel-body
                .text-center(ng-if='!vm.SelectedBankCardGroupNode')
                    label.text-danger {{"NO_BANKCARD_GROUP_MESSAGE" | translate}}
                .col-sm-12(ng-class="{'col-sm-6':(vm.groupUsed == 'PMS')}")
                    .well
                        .search-filters-item
                            .control-label {{"Bank Card Name" | translate}}
                            input.form-control(type='text', ng-model='vm.bankCardFilterOptions.bankAccountName')
                        .search-filters-item
                            .control-label {{"BANK_ACCOUNT_NUMBER" | translate}}
                            input.form-control(type='text', ng-model='vm.bankCardFilterOptions.bankAccountNumber')
                        .threeBoxes(ng-if="vm.groupUsed != 'PMS'")
                            .control-label {{"IS_IN_GROUP" | translate}}
                            select.form-control(ng-model="vm.bankCardFilterOptions.included")
                                option(value='all') {{'any' | translate}}
                                option(value='yes') {{'IN_GROUP' | translate}}
                                option(value='no') {{'NOT_IN_GROUP' | translate}}
                        .text-right.widthto100
                            button.btn.btn-primary(data-dismiss='modal', ng-click="vm.bankCardsFilter()") {{"FILTER" | translate}}
                .col-sm-6(ng-if="vm.groupUsed == 'PMS'")
                    .well
                        .search-filters-item
                            .control-label {{"Player's Name (PMS Group Search)" | translate}}
                            input.form-control(type='text', ng-model='vm.pmsGroupPlayerName')
                        .text-right.widthto100
                            button.btn.btn-primary(data-dismiss='modal', ng-click="vm.playerPMSBankCardGroupSearch()") {{"SEARCH" | translate}}

                span {{"All Pages" | translate}}: {{vm.totalBankCardShows()}}
                table#bankCards.table(ng-if='vm.SelectedBankCardGroupNode')
                    thead
                        tr
                            th(ng-if='vm.groupUsed != "PMS"') {{'SELECT' | translate}}
                            th(data-toggle="modal", data-target="#modalFilterBankCardStatus", style="cursor: pointer")
                                u {{'STATUS' | translate}}
                            th(data-toggle="modal", data-target="#modalFilterBankCardType", style="cursor: pointer")
                                u {{'bankCardType' | translate}}
                            th {{'Bank Card Name' | translate}}
                            th {{'BANK_ACCOUNT_NUMBER' | translate}}
                            th {{'BANK_ACCOUNT_PROVINCE' | translate}}
                            th {{'BANK_ACCOUNT_CITY' | translate}}
                            th {{'BANK_ACCOUNT_BRANCH' | translate}}
                            th {{'SINGLE_LIMIT' | translate}}
                            th {{'DAY_LIMIT' | translate}}
                    tbody
                        tr(ng-repeat="v in vm.allBankCards" ng-class="{'bg-secondary': !v.included}" ng-show="v.show$")
                            td(ng-if='vm.groupUsed != "PMS"')
                                input(type="checkbox" ng-model="v.selected")
                            td.text-center(ng-class="{'text-disabled': v.state === 'DISABLED'}") {{v.status === 'DISABLED' ? 'DISABLE' : v.status | translate}}
                            td {{v.bankTypeId ? vm.getBankCardTypeTextbyId(v.bankTypeId) : ("VOID" | translate)}}
                            td {{v.name}}
                            td {{v.accountNumber}}
                            td {{v.provinceName}}
                            td {{v.cityName}}
                            td {{v.openingPoint}}
                            td {{v.singleLimit}}
                            td {{v.dailyLimit}}


                //#bankList.panel-group(ng-if='vm.SelectedBankCardGroupNode')
