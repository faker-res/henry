                        text {{"EXCLUDE" | translate}}
                    button.btn.btn-primary.btn-space(ng-click="vm.addBankCardToGroup()")
                        i.fa.fa-plus-square.symbol
                        text {{"INCLUDE" | translate}}
                    button.btn.btn-primary.btn-space(ng-if="vm.selectedPlatform.data.financialSettlement && vm.selectedPlatform.data.financialSettlement.financialSettlementToggle && checkViewPermission('Payment', 'BankCardGroup', 'EditBankCard')  && vm.allBankCards.length > 0" ng-click="vm.renderEditBankCardInfo()" ng-disabled="vm.disableEditBankCard()", data-toggle="modal", data-target='#modalEditBankCard')
