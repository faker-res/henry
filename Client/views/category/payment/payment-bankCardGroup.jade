#modalAddNewBankCard.modal.fade(tabindex='-1', role='dialog', data-backdrop='static', keyboard=false,aria-labelledby='myModalLabel', aria-hidden='true')
    .modal-dialog
        .modal-content
            .modal-header
                a.fa.fa-times.close(type='button', data-dismiss='modal')
                h4.modal-title {{"Add Bank Card" | translate}}
            .modal-body
                .panel.panel-primary
                    .panel-body
                        .form-group.col-md-6
                            .control-label {{"STATUS" | translate}}
                                select.form-control(ng-model='vm.newBankCardAcc.status')
                                    option(ng-repeat='(i,v) in vm.paymentListState', value='{{v}}') {{v | translate}}
                        .form-group.col-md-6
                            .control-label {{"BANK_NAME" | translate}}
                                select.form-control(ng-model='vm.newBankCardAcc.bankTypeId')
                                    option(ng-repeat='(i,v) in vm.cloneAllBankTypeList', value='{{v.bankTypeId}}') {{v.name}}
                        .form-group.col-md-6
                            .control-label {{"BANK_CARD_NAME" | translate}}
                            input.form-control(type='text', ng-model='vm.newBankCardAcc.name')
                        .form-group.col-md-6
                            .control-label {{"BANK_CARD_ACCOUNT_NUMBER" | translate}}
                            input.form-control(type='text', ng-model='vm.newBankCardAcc.accountNumber')
                        .form-group.col-md-6
                            .control-label {{"BANK_ACCOUNT_PROVINCE" | translate}}
                            select.form-control(ng-model='vm.newBankCardAcc.provinceId', ng-change='vm.changeProvince(vm.newBankCardAcc)')
                                option(ng-repeat='(i,v) in vm.provinceList', value='{{v.id}}') {{v.name}}
                        .form-group.col-md-6
                            .control-label {{"BANK_ACCOUNT_CITY" | translate}}
                            select.form-control(ng-model='vm.newBankCardAcc.cityId')
                                option(ng-repeat='(i,v) in vm.cityList', value='{{v.id}}') {{v.name}}
                        .form-group.col-md-6
                            .control-label {{"BANK_ACCOUNT_BRANCH" | translate}}
                            input.form-control(type='text', ng-model='vm.newBankCardAcc.openingPoint')
                        .form-group.col-md-6
                            .control-label {{"SINGLE_LIMIT" | translate}}
                            input.form-control(type='Number', ng-model='vm.newBankCardAcc.maxDepositAmount', style='text-align:left!important')
                        .form-group.col-md-6
                            .control-label {{"DAY_LIMIT" | translate}}
                            input.form-control(type='Number', ng-model='vm.newBankCardAcc.quota', style='text-align:left!important')
            .modal-footer
                button.btn.btn-primary.common-button(data-dismiss='modal', ng-click="vm.createNewBankCardAcc()", ng-disabled="vm.validateCreateNewBankCard()") {{"CONFIRM" | translate}}
                button.btn.btn-warning.common-button(data-dismiss='modal') {{"CANCEL" | translate}}

#modalEditBankCard.modal.fade(tabindex='-1', role='dialog', data-backdrop='static', keyboard=false,aria-labelledby='myModalLabel', aria-hidden='true')
    .modal-dialog
        .modal-content
            .modal-header
                a.fa.fa-times.close(type='button', data-dismiss='modal')
                h4.modal-title {{"EditBankCard" | translate}}
            .modal-body
                .panel.panel-primary
                    .panel-body
                        .form-group.col-md-6
                            .control-label {{"STATUS" | translate}}
                                select.form-control(ng-model='vm.selectedBankCard.status')
                                    option(ng-repeat='(i,v) in vm.paymentListState', value='{{v}}') {{v === "DISABLED" ? "DISABLE" : v | translate}}
                        .form-group.col-md-6
                            .control-label {{"BANK_NAME" | translate}}
                                select.form-control(ng-model='vm.selectedBankCard.bankTypeId')
                                    option(ng-repeat='(i,v) in vm.cloneAllBankTypeList', value='{{v.bankTypeId}}') {{v.name}}
                        .form-group.col-md-6
                            .control-label {{"BANK_CARD_NAME" | translate}}
                            input.form-control(type='text', ng-model='vm.selectedBankCard.name')
                        .form-group.col-md-6
                            .control-label {{"BANK_CARD_ACCOUNT_NUMBER" | translate}}
                            input.form-control(type='text', ng-model='vm.selectedBankCard.accountNumber')
                        .form-group.col-md-6
                            .control-label {{"BANK_ACCOUNT_PROVINCE" | translate}}
                            select.form-control(ng-model='vm.selectedBankCard.provinceId', ng-change='vm.changeProvince(vm.selectedBankCard)')
                                option(ng-repeat='(i,v) in vm.provinceList', value='{{v.id}}') {{v.name}}
                        .form-group.col-md-6
                            .control-label {{"BANK_ACCOUNT_CITY" | translate}}
                            select.form-control(ng-model='vm.selectedBankCard.cityId')
                                option(ng-repeat='(i,v) in vm.cityList', value='{{v.id}}') {{v.name}}
                        .form-group.col-md-6
                            .control-label {{"BANK_ACCOUNT_BRANCH" | translate}}
                            input.form-control(type='text', ng-model='vm.selectedBankCard.openingPoint')
                        .form-group.col-md-6
                            .control-label {{"SINGLE_LIMIT" | translate}}
                            input.form-control(type='Number', ng-model='vm.selectedBankCard.maxDepositAmount', style='text-align:left!important')
                        .form-group.col-md-6
                            .control-label {{"DAY_LIMIT" | translate}}
                            input.form-control(type='Number', ng-model='vm.selectedBankCard.quota', style='text-align:left!important')
            .modal-footer
                button.btn.btn-primary.common-button(data-dismiss='modal', ng-click="vm.editBankCardAcc()", ng-disabled="vm.validateEditBankCard()") {{"CONFIRM" | translate}}
                button.btn.btn-warning.common-button(data-dismiss='modal', ng-click="vm.cancelEditBankCardAcc()") {{"CANCEL" | translate}}
                button.btn.btn-danger.common-button(data-dismiss='modal', ng-click="vm.deleteBankCardAcc()") {{"DELETE" | translate}}

