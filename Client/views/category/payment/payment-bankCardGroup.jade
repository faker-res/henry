                                        tr
                                            td.text-center
                                            td {{'bankCardType' | translate}}
                                            td.text-success {{v.bankTypeId ? vm.getBankCardTypeTextbyId(v.bankTypeId) : ("VOID" | translate)}}
                                        tr
                                            td.text-center
                                            td {{'Bank Card Name' | translate}}
                                            td.text-success {{v.name}}
                                        tr
                                            td.text-center
                                            td {{'BANK_ACCOUNT_NUMBER' | translate}}
                                            td.text-success {{v.accountNumber}}
                                        tr
                                            td.text-center
                                            td {{'BANK_ACCOUNT_PROVINCE' | translate}}
                                            td.text-success {{v.provinceName}}
                                        tr
                                            td.text-center
                                            td {{'BANK_ACCOUNT_CITY' | translate}}
                                            td.text-success {{v.cityName}}
                                        tr
                                            td.text-center
                                            td {{'BANK_ACCOUNT_BRANCH' | translate}}
                                            td.text-success {{v.openingPoint}}
                                        tr
                                            td.text-center
                                            td {{'DAY_LIMIT' | translate}}
                                            td.text-success {{v.quota}}

                                        //tr
                                        //    td.text-center(rowspan="3")
                                        //        i.fa.fa-credit-card.text-danger
                                        //    td {{'TITLE' | translate}}
                                        //    td.text-success {{v.name}}
                                        //tr
                                        //    td {{'ACCOUNT' | translate}}
                                        //    td.text-success {{v.accountNumber}}
                                        //tr
                                        //    td {{'BANK_TYPE' | translate}}
                                        //    td.text-success {{v.bankTypeId ? vm.getBankCardTypeTextbyId(v.bankTypeId) : ("VOID" | translate)}}
                                        //tr
                                        //    td {{'QUOTA' | translate}}
                                        //    td.text-success {{v.quota}}
                    .panel.panel-default
                        .panel-heading.panel-heading-sm(data-toggle='collapse', data-parent="#bankList", href='#excludedBanks', ng-click='vm.toggleBankType("exclude")')
                            h4.panel-title
                                text {{'Excluded Bankcards' | translate}}
                                span(ng-if='vm.excludedBanks') &nbsp;:&nbsp;({{vm.excludedBanks.length}})
                                text.blink.margin-left-5.text-danger(ng-if='!vm.excludedBanks') {{'loading bankcards' | translate}}...
                        #excludedBanks.panel-collapse.collapse
                            .panel-body(style='height:250px;overflow-y:auto')
                                a.platformBankcardDiv(ng-repeat='(i,v) in vm.excludedBanks', class='{{vm.highlightBank[v.accountNumber]}}', ng-click='vm.bankClicked(i,v,"ex")')
                                    table.bg-pale
                                        tr
                                            td.text-center
                                                td {{'STATUS' | translate}}
                                                td(ng-class="{'text-disabled': v.status === 'DISABLED'}").text-success {{v.status| translate}}
                                        tr
                                            td.text-center
                                            td {{'bankCardType' | translate}}
                                            td.text-success {{v.bankTypeId ? vm.getBankCardTypeTextbyId(v.bankTypeId) : ("VOID" | translate)}}
                                        tr
                                            td.text-center
                                            td {{'Bank Card Name' | translate}}
                                            td.text-success {{v.name}}
                                        tr
                                            td.text-center
                                            td {{'BANK_ACCOUNT_NUMBER' | translate}}
                                            td.text-success {{v.accountNumber}}
                                        tr
                                            td.text-center
                                            td {{'BANK_ACCOUNT_PROVINCE' | translate}}
                                            td.text-success {{v.provinceName}}
                                        tr
                                            td.text-center
                                            td {{'BANK_ACCOUNT_CITY' | translate}}
                                            td.text-success {{v.cityName}}
                                        tr
                                            td.text-center
                                            td {{'BANK_ACCOUNT_BRANCH' | translate}}
                                            td.text-success {{v.openingPoint}}
                                        tr
                                            td.text-center
                                            td {{'DAY_LIMIT' | translate}}
                                            td.text-success {{v.quota}}
                                        //tr
                                        //    td.text-center(rowspan="4")
                                        //        i.fa.fa-credit-card.text-danger
                                        //    td {{'TITLE' | translate}}
                                        //    td.text-success {{v.name}}
                                        //tr
                                        //    td {{'ACCOUNT' | translate}}
                                        //    td.text-success {{v.accountNumber}}
                                        //tr
                                        //    td {{'BANK_TYPE' | translate}}
                                        //    td.text-success {{v.bankTypeId ? vm.getBankCardTypeTextbyId(v.bankTypeId) : ("VOID" | translate)}}
                                        //tr
                                        //    td {{'QUOTA' | translate}}
                                        //    td.text-success {{v.quota}}
                //.panel.panel-info(ng-if='vm.curBank')
                    .panel-heading.panel-heading-sm
                        h4.panel-title(style='display:inline-block') {{'Bankcard' | translate}} : {{vm.curBank.name}}
                        //label.pull-right(ng-if='vm.curBank') {{'BANK_TYPE' | translate}}&nbsp;:&nbsp;{{vm.curBank.bankTypeId ? vm.curBank.bankTypeId : ("VOID" | translate)}}
                    .panel-body(style='height:80px;padding:5px')
                        .textareaDetail
                            textarea(disabled, readonly, rows="3" style="border:none") {{'BANK_TYPE' | translate}}&nbsp;:&nbsp;{{vm.curBank.bankTypeId ? vm.getBankCardTypeTextbyId(vm.curBank.bankTypeId) : ("VOID" | translate)}}
                div.text-right.widthto100(ng-if='vm.SelectedBankCardGroupNode && checkViewPermission("Payment", "BankCardGroup", "Update") && vm.groupUsed != "PMS"')
                    button.btn.btn-info.btn-space(ng-click="vm.selectBankCard(true)") {{"Select All" | translate}}
                    button.btn.btn-info.btn-space(ng-click="vm.selectBankCard(false)") {{"Clear All" | translate}}
                    button.btn.btn-primary.btn-space(ng-click="vm.removeBankCardFromGroup()")
