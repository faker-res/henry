.section(ng-show="checkViewPermission('Analysis','Analysis','ApiResponseTime')")
    .row
        .titlePic
            .title {{"ApiResponseTime" | translate}}
                .pull-right
                    a.fa.fa-cog(ng-click='vm.toggleGraphSettingPanel("#allApiResponseTimeAnalysis .block-query","allApiResponseTime")')
                        text.pull-left.margin-right-5 {{vm.optionText.allApiResponseTime | translate}}
    #allApiResponseTimeAnalysis
        .col-md-12.block-query.well(style='margin-bottom:3px;')
            div
                label {{"Service Name" | translate}}
                select.form-control(ng-model='vm.queryPara.allApiResponseTime.service')
                    option(ng-repeat='i in vm.apiRespServiceNames track by $index') {{i}}
            div
                label {{"Function Name" | translate}}
                select.form-control(ng-model='vm.queryPara.allApiResponseTime.funcName')
                    option(ng-repeat='i in vm.apiRespFuncNames track by $index') {{i}}
            div
                label {{"Provider" | translate}}
                select.form-control(ng-model='vm.queryPara.allApiResponseTime.providerId', style='max-width:100px')
                    option(value='') {{'any' | translate}}
                    option(ng-repeat='(i,v) in vm.allGameProvider track by $index', value='{{v.providerId}}') {{v.name}}({{v.providerId}})
            div
                label {{"Start date"|translate}}
                .startTime
            div
                label {{"End date"|translate}}
                .endTime
            //div
            //    label {{"Start date"|translate}}
            //    input.form-control(type='date', ng-model='vm.queryPara.allApiResponseTime.startTime')
            //div
            //    label {{"End date"|translate}}
            //    input.form-control(type='date', ng-model='vm.queryPara.allApiResponseTime.endTime')
            div
                label &nbsp;
                button.btn.btn-primary.common-button(ng-click='vm.plotAllPlatformApiResponseTime()') {{'SEARCH' | translate}}
            div.collapse
                label &nbsp;
                i.fa.fa-spin.fa-spinner
        .row
            .col-md-12
                .graph
                    .panel.panel-default.rightPanel
                        #line-all-apiResponseTime.lineChart
p(ng-init="vm.loadPageFunc('PLATFORM_OVERVIEW')")