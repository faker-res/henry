.section
    .row
        .titlePic
            .title {{"ACTIVE_PLAYER" | translate}}
                .pull-right
                    a.fa.fa-cog(ng-click='vm.toggleGraphSettingPanel("#allActivePlayerAnalysis .block-query","allActivePlayer")')
                        text.pull-left.margin-right-5 {{vm.optionText.allActivePlayer | translate}}
    #allActivePlayerAnalysis
        .col-md-12.block-query.well(style='margin-bottom:3px;')
            div
                label {{"date"|translate}}
                input.form-control(type='date', ng-model='vm.queryPara.allActivePlayer.date')
            div
                label &nbsp;
                button.btn.btn-primary.common-button(ng-click='vm.plotAllPlatformActivePlayerPie()') {{'SEARCH' | translate}}
        .row
            .col-md-5
                .panel.panel-default.leftPanel(style='padding-bottom:5px')
                    .pieChart
                        #pie-all-activePlayer
            .col-md-7
                .panel.panel-default.rightPanel
                    #bar-all-activePlayer.barChart
                    label {{vm.allPlatformActivePie}}
.clearfix
