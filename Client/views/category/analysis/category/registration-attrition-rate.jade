                                th(ng-click="vm.dataArraySort( 'H5', 'date')") {{ vm.queryPara.registrationAttritionRate.periodText | translate}}
                                th(ng-click="vm.dataArraySort('H5','H5Count')") H5 {{"Number Of Successful Registration"| translate}}
                                th(ng-click="vm.dataArraySort('H5','H5UniqueCount')") H5 {{'Clicking Rate (Non-repeated IP)' | translate}}
                                th(ng-click="vm.dataArraySort('H5','H5AttritionRate$')") H5 {{'Registration Attrition Rate' | translate}}
                        tbody
                            tr
                                td
                                    span {{ "average value" | translate }}
                                td
                                    span {{ vm.H5Registration.avgCount }}
                                td
                                    span {{ vm.H5Registration.uniqueAvgCount }}
                                td
                                    span {{ vm.H5Registration.attritionAvgRate$ }} %
                            tr(ng-repeat='(i, v) in vm.registrationRecord | orderBy: vm.dataSort.H5')
                                td
                                    span {{ v.date | date:"yyyy-MM-dd" }}
