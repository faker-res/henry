                .graph
                    table.table-striped.display.tableDiv.no-footer.dataTable.text-center(style='width:100%')
                        thead
                            tr
                                th(ng-click="vm.dataArraySort( 'frontEnd', 'date')") {{ vm.queryPara.registrationAttritionRate.periodText | translate}}
