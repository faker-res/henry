.section
    .row
        .titlePic
            .title
                text {{vm.showPageName | translate}}
                .pull-right
                    a.fa.fa-cog(ng-click='vm.toggleGraphSettingPanel("#registrationAttritionRateAnalysis .block-query","registrationAttritionRate")')
                        text.pull-left.margin-right-5 {{vm.optionText.registrationAttritionRate | translate}}
    #registrationAttritionRateAnalysis
        .col-md-12.block-query.well(style='margin-bottom:3px;')
            div
                label {{'PERIOD' | translate}}
                select.form-control(ng-model='vm.queryPara.registrationAttritionRate.periodText')
                    option(value='day') {{"DAY"|translate}}
                    option(value='week') {{"WEEK"|translate}}
                    option(value='month') {{"MONTH"|translate}}
            div
                label {{"Start date"|translate}}
                .startTime
            div
                label {{"End date"|translate}}
                .endTime
            div
                label &nbsp;
                button.btn.btn-primary.common-button(ng-click='vm.drawFrontEndRegistrationAttritionRate();') {{'SEARCH' | translate}}
        .row
            .col-md-6
                label.text-primary WEB {{"REGISTRATION_ATTRITION_RATE" | translate}} VS {{vm.queryPara.registrationAttritionRate.periodText | translate}}
                .graph
                    .panel.panel-default
                        #line-webRegistrationAttritionRate.lineChart.graphDiv
            .col-md-6
                label &nbsp;
                //label.text-primary {{vm.queryPara.topUp.amountTag | translate}} VS {{vm.platformTopUpDataPeriodText | translate}}
                .graph
                    table.table-striped.display.tableDiv.no-footer.dataTable.text-center(style='width:100%')
                        thead
                            tr
                                th(ng-click="vm.dataArraySort( 'web', 'date')") {{ vm.queryPara.registrationAttritionRate.periodText | translate}}
                                th(ng-click="vm.dataArraySort('web','webCount')") WEB {{"Number Of Successful Registration"| translate}}
                                th(ng-click="vm.dataArraySort('web','webUniqueCount')") WEB {{'Clicking Rate (Non-repeated IP)' | translate}}
                                th(ng-click="vm.dataArraySort('web','webAttritionRate$')") WEB {{'Registration Attrition Rate' | translate}}
                        tbody
                            tr
                                td
                                    span {{ "average value" | translate }}
                                td
                                    span {{ vm.webRegistration.avgCount }}
                                td
                                    span {{ vm.webRegistration.uniqueAvgCount }}
                                td
                                    span {{ vm.webRegistration.attritionAvgRate$ }} %
                            tr(ng-repeat='(i, v) in vm.registrationRecord | orderBy: vm.dataSort.web')
                                td
                                    span {{ v.date | date:"yyyy-MM-dd" }}
                                td
                                    span {{ v.webCount }}
                                td
                                    span {{ v.webUniqueCount }}
                                td
                                    span {{ v.webAttritionRate$ }} %
        .row
            .col-md-6
                label.text-primary H5 {{"REGISTRATION_ATTRITION_RATE" | translate}} VS {{vm.queryPara.registrationAttritionRate.periodText | translate}}
                .graph
                    .panel.panel-default
                        #line-H5RegistrationAttritionRate.lineChart.graphDiv
            .col-md-6
